{
  "name": "lenderoo",
  "description": "Track items you've lent to friends - books, tools, clothes, and more. Never forget who has what!",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "yarn@4.5.0",
  "private": true,
  "scripts": {
    "start": "expo start -c --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "publish:web": "yarn build:web && eas deploy --alias lenderoo-web",
    "build:web": "expo export --platform web",
    "dbmigrate": "npx supabase db push"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
