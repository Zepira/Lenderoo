{
  "name": "lenderoo",
  "description": "Track items you've lent to friends - books, tools, clothes, and more. Never forget who has what!",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "yarn@4.5.0",
  "private": true,
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "check:tamagui": "tamagui check",
    "start": "expo start -c --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "publish:web": "yarn build:web && eas deploy",
    "build:web": "expo export --platform web",
    "deploy": "gh-pages --nojekyll -d dist",
    "predeploy": "expo export -p web"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.6",
    "@rn-primitives/avatar": "^1.2.0",
    "@rn-primitives/label": "^1.2.0",
    "@rn-primitives/portal": "^1.3.0",
    "@rn-primitives/select": "^1.2.0",
    "@rn-primitives/separator": "^1.2.0",
    "@rn-primitives/slot": "^1.2.0",
    "@tamagui/config": "^1.138.0",
    "@tamagui/lucide-icons": "^1.138.0",
    "@tamagui/toast": "^1.138.0",
    "babel-preset-expo": "~54.0.0",
    "burnt": "^0.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^54.0.23",
    "expo-build-properties": "~1.0.9",
    "expo-font": "~14.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "lucide-react-native": "^0.562.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tamagui": "^1.138.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@expo/metro-config": "~54.0.2",
    "@expo/metro-runtime": "~6.1.2",
    "@tamagui/babel-plugin": "^1.144.2",
    "@tamagui/cli": "^1.138.0",
    "@tamagui/metro-plugin": "^1.138.0",
    "@types/gh-pages": "^6",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.73.0",
    "gh-pages": "^6.3.0",
    "typescript": "~5.9.2"
  }
}
